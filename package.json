  "version": "2.0.0",
  "description": "Tagsy l'éditeur de tags d'exercice de j'accorde",
  "main": "js/app.js",
  "private": "true",
  "scripts": {
    "sass": "sass --watch scss/main.scss dist/css/main.css",
    "sassMin": "sass --watch --style=compressed scss/main.scss dist/min/css/main.min.css",
    "scss": "npm run sass && npm run sassMin",
    "build": "rollup --config",
    "reload": "reload -b",
    "serv": " npm run reload",
    "gulp": "gulp",
    "commit": "git commit -a -m",
    "push" : "git push origin main"
  },
  "dependencies": {
    "reload": "^3.1.1",
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "latest",
    "gulp": "^4.0.2",
    "rollup": "^2.40.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
